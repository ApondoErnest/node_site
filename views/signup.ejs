  <script src="products/js/jquery.min.js"></script>
  <script src="products/js/jquery-migrate-3.0.1.min.js"></script>
  <script src="products/js/popper.min.js"></script>
  <script src="products/js/bootstrap.min.js"></script>
  <script src="products/js/jquery.easing.1.3.js"></script>
  <script src="products/js/jquery.waypoints.min.js"></script>
  <script src="products/js/jquery.stellar.min.js"></script>
  <script src="products/js/owl.carousel.min.js"></script>
  <script src="products/js/jquery.magnific-popup.min.js"></script>
  <script src="products/js/aos.js"></script>
  <script src="products/js/jquery.animateNumber.min.js"></script>
  <script src="products/js/bootstrap-datepicker.js"></script>
  <script src="products/js/scrollax.min.js"></script>
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBVWaKrjvy3MaE7SQ74_uJiULgl1JY0H2s&sensor=false"></script>
